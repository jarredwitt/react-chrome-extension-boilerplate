{
  "name": "react-chrome-extension-boilerplate",
  "version": "0.9.1",
  "description": "Boilerplate for Chrome extension React.js project",
  "scripts": {
    "dev": "concurrently \"npm run dev-r\" \"npm run dev-s\"",
    "dev-r": "node scripts/devReloadable",
    "dev-s": "node scripts/devStatic",
    "build": "node scripts/build",
    "compress": "node scripts/compress",
    "compress-keygen": "crx keygen",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "lint": "eslint app chrome test scripts webpack/*.js",
    "test-e2e": "cross-env NODE_ENV=test mocha -r ./test/setup-app test/e2e",
    "test": "cross-env NODE_ENV=test mocha -r ./test/setup-app test/app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jhen0409/react-chrome-extension-boilerplate.git"
  },
  "keywords": [
    "chrome",
    "extension",
    "react",
    "redux",
    "hot reload",
    "webpack",
    "boilerplate"
  ],
  "author": "Jhen <developer@jhen.me>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/react-addons-test-utils": "^0.14.21",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "chromedriver": "^73.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "crx": "^5.0.0",
    "css-loader": "^2.1.1",
    "css-modules-require-hook": "^4.2.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^3.0.1",
    "jsdom": "^15.0.0",
    "minimist": "^1.2.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.9.1",
    "postcss-loader": "^3.0.0",
    "pug-cli": "^1.0.0-alpha6",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^7.0.3",
    "selenium-webdriver": "^3.6.0",
    "shelljs": "^0.8.3",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "bluebird": "^3.3.4",
    "classnames": "^2.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.0",
    "react-dock": "^0.2.4",
    "react-dom": "^16.4.0",
    "react-intl": "^2.4.0",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}